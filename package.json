{
  "name": "zenn-metadata-updater",
  "version": "0.2.1",
  "description": "Update metadata in Zenn markdown file for npm.",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "compile": "tsc",
    "build": "npm-run-all -l -s clean compile",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm-run-all -l -p lint test -s build",
    "clean": "rimraf lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korosuke613/zenn-metadata-updater.git"
  },
  "keywords": [
    "node",
    "zenn",
    "yaml"
  ],
  "author": "Futa Hirakoba <korosuke613613@gmail.com> (https://korosuke613.github.io/)",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^4.0.0",
    "yaml-front-matter": "^4.1.1"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "18.0.0",
    "@swc/core": "1.3.21",
    "@swc/jest": "0.2.23",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/yaml-front-matter": "4.1.0",
    "eslint": "8.28.0",
    "eslint-plugin-jest": "27.1.6",
    "jest": "29.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "rimraf": "3.0.2",
    "shipjs": "0.25.0",
    "typescript": "4.9.3"
  }
}
