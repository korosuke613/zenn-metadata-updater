{
  "name": "zenn-metadata-updater",
  "version": "0.2.1",
  "description": "Update metadata in Zenn markdown file for npm.",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "compile": "tsc",
    "build": "npm-run-all -l -s clean compile",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm-run-all -l -p lint test -s build",
    "clean": "rimraf lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korosuke613/zenn-metadata-updater.git"
  },
  "keywords": [
    "node",
    "zenn",
    "yaml"
  ],
  "author": "Futa Hirakoba <korosuke613613@gmail.com> (https://korosuke613.github.io/)",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^4.0.0",
    "yaml-front-matter": "^4.1.1"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^17.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/yaml-front-matter": "^4.1.0",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "^26.1.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "shipjs": "^0.24.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
